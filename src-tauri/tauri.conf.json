{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "retropulse",
  "version": "0.1.0",
  "identifier": "de.westhoffswelt.retropulse",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 200,
        "title": "RetroPulse",
        "width": 430,
        "alwaysOnTop": true,
        "closable": false,
        "decorations": false,
        "dragDropEnabled": false,
        "focus": false,
        "hiddenTitle": true,
        "label": "main",
        "maximizable": false,
        "minimizable": false,
        "resizable": false,
        "shadow": false,
        "visible": true,
        "transparent": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  }
}
